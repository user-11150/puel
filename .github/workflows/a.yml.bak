name: get token

on:
  push:

permissions:
  contents: write
  id-token: write
  pages: write

jobs:
  GetToken:
    runs-on: ubuntu-latest
    steps:
      - name: echo token
        env:
          TWINE_USERNAME: __token__
          TWINE_PASSWORD: ${{secrets.PYPI_TOKEN}}
        run: |
          echo ${TWINE_PASSWORD}
