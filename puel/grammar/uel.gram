stmts:
    ->: <stmt>+
stmt:
    ->: <expr>
    ->: <put>
    ->: <push>
    ->: <if>

constant:
    ->: (\d|\.)+
    ->: ".+"
    ->: true | false
    ->: TOP

expr:
    ->: <constant>
    ->: <expr>\s*[+-*/]\s*<expr>

put:
    ->: PUT <expr>

push:
    ->: PUSH <expr>

if:
    ->: if <expr> <stmts>
    ->: if <expr> <stmts> END
    ->: if <expr> <stmts> END ELSE <stmts>
    ->: if <expr> <stmts> END ELSE <stmts> END
    
